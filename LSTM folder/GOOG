```{r, warning=FALSE, results='hide'}
model_GOOG_t10_L4_U64_U64_U32_U32_B64_D02 <- keras_model_sequential() %>%
  # LSTM layer
  layer_lstm(units = 64, 
             dropout=0.2, 
             recurrent_dropout=0.2,
             input_shape = c(timesteps, 1),
             return_sequences = TRUE) %>%
    # A Dense layer inbetweeen
  layer_dense(units = 32, activation = 'relu') %>%
  layer_lstm(units = 64, 
             dropout=0.2, 
             recurrent_dropout=0.2,
             return_sequences = TRUE) %>%
  layer_dense(units = 32, activation = 'relu') %>%
  layer_lstm(units = 32, 
             dropout=0.2, 
             recurrent_dropout=0.2,
             return_sequences = TRUE) %>%
    layer_dense(units = 32, activation = 'relu') %>%
  layer_lstm(units = 32, 
             dropout=0.2, 
             recurrent_dropout=0.2,
             return_sequences = FALSE) %>%
  layer_dense(units = 32, activation = 'relu') %>%
  #Final prediction layer
  layer_dense(units = 1, activation = 'linear')

model_GOOG_t10_L4_U64_U64_U32_U32_B64_D02 %>% compile(loss = 'mse', optimizer = optimizer_adam(), metrics = 'mse')

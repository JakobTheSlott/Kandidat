# Evaluering af modeller 10 time-steps


### Evaluering 2 Layers og 10  time-steps

```{r, include = FALSE}
# 2 Layers, 10 time-steps, drop-out rate 0.2 og batch size 16
eval_GOOG_10t_L2_U128_U128_B16_D02 <- model_GOOG_t10_L2_U128_U128_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U128_U64_B16_D02 <- model_GOOG_t10_L2_U128_U64_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U64_U64_B16_D02 <- model_GOOG_t10_L2_U64_U64_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U64_U32_B16_D02 <- model_GOOG_t10_L2_U64_U32_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U32_U32_B16_D02 <- model_GOOG_t10_L2_U32_U32_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U32_U16_B16_D02 <- model_GOOG_t10_L2_U32_U16_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U16_U16_B16_D02 <- model_GOOG_t10_L2_U16_U16_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U16_U8_B16_D02 <- model_GOOG_t10_L2_U16_U8_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U8_U8_B16_D02 <- model_GOOG_t10_L2_U8_U8_B16_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)


# 2 Layers, 10 time-steps, drop-out rate 0.2 og batch size 32
eval_GOOG_10t_L2_U128_U128_B32_D02 <- model_GOOG_t10_L2_U128_U128_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U128_U64_B32_D02 <- model_GOOG_t10_L2_U128_U64_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U64_U64_B32_D02 <- model_GOOG_t10_L2_U64_U64_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U64_U32_B32_D02 <- model_GOOG_t10_L2_U64_U32_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U32_U32_B32_D02 <- model_GOOG_t10_L2_U32_U32_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U32_U16_B32_D02 <- model_GOOG_t10_L2_U32_U16_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U16_U16_B32_D02 <- model_GOOG_t10_L2_U16_U16_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U16_U8_B32_D02 <- model_GOOG_t10_L2_U16_U8_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)
eval_GOOG_10t_L2_U8_U8_B32_D02 <- model_GOOG_t10_L2_U8_U8_B32_D02 %>% evaluate(x_train_arr_GOOG1, y_train_arr_GOOG1)


# 2 Layers, 10 time-steps, drop-out rate 0.2 og batch size 64

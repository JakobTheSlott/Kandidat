eval_GOOG_1t_L4_U16_U16_U16_U16_B32_D03 <- model_GOOG_t1_L4_U16_U16_U16_U16_B32_D03 %>% evaluate(x_train_arr_GOOG, y_train_arr_GOOG)
eval_GOOG_1t_L4_U16_U16_U16_U8_B32_D03 <- model_GOOG_t1_L4_U16_U16_U16_U8_B32_D03 %>% evaluate(x_train_arr_GOOG, y_train_arr_GOOG)
eval_GOOG_1t_L4_U16_U16_U8_U8_B32_D03 <- model_GOOG_t1_L4_U16_U16_U8_U8_B32_D03 %>% evaluate(x_train_arr_GOOG, y_train_arr_GOOG)
eval_GOOG_1t_L4_U16_U8_U8_U8_B32_D03 <- model_GOOG_t1_L4_U16_U8_U8_U8_B32_D03 %>% evaluate(x_train_arr_GOOG, y_train_arr_GOOG)


# 4 Layers, 1 time-steps, drop-out rate 0.3 og batch size 64
eval_GOOG_1t_L4_U128_U128_U128_U128_B64_D03 <- model_GOOG_t1_L4_U128_U128_U128_U128_B64_D03 %>% evaluate(x_train_arr_GOOG, y_train_arr_GOOG)

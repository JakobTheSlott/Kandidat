  layer_dense(units = 32, activation = 'relu') %>%
  layer_lstm(units = 64, 
             dropout=0.3, 
             recurrent_dropout=0.3,
             return_sequences = TRUE) %>%
  layer_dense(units = 32, activation = 'relu') %>%
  layer_lstm(units = 64, 
             dropout=0.3, 
             recurrent_dropout=0.3,
             return_sequences = TRUE) %>%
    layer_dense(units = 32, activation = 'relu') %>%
  layer_lstm(units = 64, 
             dropout=0.3, 
             recurrent_dropout=0.3,
             return_sequences = FALSE) %>%
  layer_dense(units = 32, activation = 'relu') %>%
  #Final prediction layer
  layer_dense(units = 1, activation = 'linear')

model_GOOG_t10_L4_U64_U64_U64_U64_B32_D03 %>% compile(loss = 'mse', optimizer = optimizer_adam(), metrics = 'mse')
```



